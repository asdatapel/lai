fib : (n : i32) -> i32 : (n : i32) -> i32 {
    if n != 0 {    
        if n != 1 {
            return fib(n - 1) + fib(n - 2);
        }
    }
    return n;
};

fib_indirect := (n : i32, out : *i32) -> i32 {
    .out = fib(n);
    return n;
};

